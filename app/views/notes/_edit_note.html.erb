<div class="edit-note">
  <%= form_with model: [@folder, @note], class: 'form', local: true do |f| %>
    <div class="form-input">
        <%= f.text_field :title, class: 'form-title', placeholder: 'タイトルを入力' %>
        <%= f.text_area :content, class: 'form-message', placeholder: '本文を入力' %>
    </div>
    <%= f.submit '保存', class: 'form-submit' %>
  <% end %>
</div>
